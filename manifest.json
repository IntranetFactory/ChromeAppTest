{
  "manifest_version": 2,
  "name": "Chrome App Test",
  "version": "1.0.0.4",
  "icons": {
    "19":"img/icon-64px.png",
    "38":"img/icon-64px.png",
    "64":"img/icon-64px.png",
    "128":"img/icon-64px.png"
  },
  "browser_action": {
    "default_icon": {
      "19":"img/error-icon-64px.png",
      "38":"img/error-icon-64px.png"
    },
    "default_popup": "html/index.html"
  },
  "permissions": [
    "storage",
    "cookies",
    "http://*/*",
    "https://*/*"
   ],
  "options_page": "html/options.html",
  "background": {
    "page": "html/background.html"
  }
}
